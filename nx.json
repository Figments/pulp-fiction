{
    "npmScope": "dragonfish",
    "affected": {
        "defaultBase": "master"
    },
    "implicitDependencies": {
        "workspace.json": "*",
        "package.json": {
            "dependencies": "*",
            "devDependencies": "*"
        },
        "tsconfig.base.json": "*",
        "tslint.json": "*",
        "nx.json": "*"
    },
    "tasksRunnerOptions": {
        "default": {
            "runner": "@nrwl/workspace/tasks-runners/default",
            "options": {
                "cacheableOperations": ["build", "lint", "test", "e2e"]
            }
        }
    },
    "projects": {
        "client": {
            "tags": []
        },
        "client-e2e": {
            "tags": [],
            "implicitDependencies": ["client"]
        },
        "server": {
            "tags": []
        },
        "word_counter": {
            "tags": []
        },
        "html_sanitizer": {
            "tags": []
        },
        "models": {
            "tags": []
        },
        "icons": {
            "tags": []
        },
        "material": {
            "tags": []
        }
    },
    "workspaceLayout": {
        "appsDir": "packages",
        "libsDir": "libraries"
    }
}
